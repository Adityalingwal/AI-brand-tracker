{
  "actorSpecification": 1,
  "name": "ai-brand-visibility",
  "title": "AI Brand Visibility",
  "description": "Track your brand's visibility across AI platforms (ChatGPT, Gemini, Perplexity). Analyze mentions, citations, and competitive positioning.",
  "version": "1.0",
  "buildTag": "latest",
  "meta": {
    "templateId": "python-playwright"
  },
  "input": "./input_schema.json",
  "dockerfile": "../Dockerfile",
  "storages": {
    "dataset": "./dataset_schema.json"
  },
  "output": "./output_schema.json",
  "defaultRunOptions": {
    "timeoutSecs": 900,
    "memoryMbytes": 4096
  },
  "pricingModel": {
    "pricingType": "PAY_PER_EVENT",
    "events": [
      {
        "eventName": "brand-analysis",
        "displayName": "Brand Analysis",
        "pricePerEvent": 0.3,
        "pricePerEventByTier": {
          "FREE": 0.3,
          "BRONZE": 0.25,
          "SILVER": 0.2,
          "GOLD": 0.15
        },
        "description": "Complete brand visibility analysis across all platforms"
      }
    ]
  }
}
