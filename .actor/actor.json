{
  "actorSpecification": 1,
  "name": "ai-brand-tracker",
  "title": "AI Brand Tracker",
  "description": "Track your brand's visibility across AI platforms (ChatGPT, Gemini, Perplexity). Analyze mentions, citations, and competitive positioning.",
  "version": "1.0.0",
  "buildTag": "latest",
  "meta": {
    "templateId": "python-playwright"
  },
  "input": "./input_schema.json",
  "dockerfile": "../Dockerfile",
  "storages": {
    "dataset": "./dataset_schema.json"
  },
  "pricingModel": {
    "pricingType": "PAY_PER_EVENT",
    "events": [
      {
        "eventName": "prompt-analyzed",
        "displayName": "Prompt Analyzed",
        "pricePerEvent": 0.20,
        "pricePerEventByTier": {
          "FREE": 0.20,
          "BRONZE": 0.18,
          "SILVER": 0.15,
          "GOLD": 0.12
        }
      }
    ]
  }
}
